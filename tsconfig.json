{
    "$schema": "https://json.schemastore.org/tsconfig",
    "compilerOptions": {
        "lib": [
            "ES2023",
            "DOM"
        ],
        "module": "ESNext",
        "target": "ES2022",
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "dst"
    },
    "include": [
        "src/**/*.ts"
    ]
}
